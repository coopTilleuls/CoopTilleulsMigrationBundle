{
    "name": "tilleuls/migration-bundle",
    "type": "symfony-bundle",
    "description": "Provides features for continuous migration.",
    "keywords": [
        "Migration",
        "Loaders",
        "Transformer"
    ],
    "license": "MIT",
    "homepage": "https://les-tilleuls.coop",
    "authors": [
        {
            "name": "Vincent Chalamon",
            "email": "vincent@les-tilleuls.coop"
        }
    ],
    "require": {
        "php": ">=7.2",
        "doctrine/annotations": "^1.2",
        "doctrine/dbal": "^2.5",
        "doctrine/doctrine-bundle": "^1.7.2 || ^2.0",
        "doctrine/orm": "^2.6.3",
        "psr/log": "^1.1",
        "symfony/config": "^3.4 || ^4.4 || ^5.0",
        "symfony/console": "^3.4 || ^4.4 || ^5.0",
        "symfony/dependency-injection": "^3.4 || ^4.4 || ^5.0",
        "symfony/doctrine-bridge": "^3.4 || ^4.4 || ^5.0",
        "symfony/http-kernel": "^3.4 || ^4.4 || ^5.0",
        "symfony/stopwatch": "^3.4 || ^4.4 || ^5.0"
    },
    "require-dev": {
        "behat/behat": "^3.1",
        "doctrine/data-fixtures": "^1.2",
        "friends-of-behat/symfony-extension": "^2.0.10@beta",
        "friendsofphp/php-cs-fixer": "^2.15.5",
        "phpspec/prophecy": "^1.10",
        "phpstan/phpstan": "^0.12.17",
        "phpunit/phpunit": "^8.0",
        "symfony/browser-kit": "^3.4 || ^4.4 || ^5.0",
        "symfony/framework-bundle": "^3.4 || ^4.4 || ^5.0",
        "zendframework/zend-code": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "CoopTilleuls\\MigrationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CoopTilleuls\\MigrationBundle\\Tests\\": "features/app"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
